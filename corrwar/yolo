p64:	ld	%10,r8
	sub	r6,r8,r6
	and	r6,%2147483648,r8
	zjmp	%:cont
	ld	%118,r8
	add	r6,r8,r6
cont:	add	r6,r6,r6
	ld	%272,r8
	ld	%29,r9
	ldi	%:p64deb,r6,r7
	sti	r7,r8,r6
	sti	r7,r9,r6
p64li:	live	%1
	ld	%0,r10
	zjmp	%186

tirdow:	ld	%425,r2
	ld	%0,r3
	ld	%4,r4

tird:	live	%42
	sti	r10,%:fin,r2
	sti	r10,%:fin,r3
	add	r4,r5,r4
	add	r3,r4,r3
	sub	r2,r4,r2
	zjmp	%:tirdow
	ld	%0,r11
	zjmp	%:tird
	zjmp	%:p1

fin:	live	%633

